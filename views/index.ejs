<% include header %>
<% if (entries.length) { %>
    <% entries.forEach(function(entry) { %> 
        <div class="panel panel-default">
            <div class="text-muted pull-right">
                <%= entry.published %>
            </div>
            <%entry.title %>
        </div>
        <div class="panel-body">
            <%= entry.body %>
        </div>
    <% }) %>
<% } else { %> 
        No entries! <a href="/new-entry">Add one!</a> 
<% } %>
<% include footer %>  
